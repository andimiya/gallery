<h1>Login</h1>


<form action="/login" method="post" class="form-container">
    <div class="username">
        <label>Username:</label>
        <input type="text" name="username"/>
    </div>
    <div class="password">
        <label>Password:</label>
        <input type="password" name="password"/>
    </div>
    <div class="submit">
        <input type="submit" value="Log In"/>
    </div>
</form>