<h1>Create User</h1>

<form id="form" action="/gallery/new-user" method="POST">
    <div class="username">
        <label>Username:</label>
        <input type="text" name="username"/>
    </div>
    <div class="password">
        <label>Password:</label>
        <input type="text" name="password"/>
    </div>
    <div class="submit">
        <input type="submit" value="Create Account"/>
    </div>
</form>

